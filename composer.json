{
  "name": "iampersistent/simple-shop",
  "type": "library",
  "description": "A small feature set ecommerce library",
  "keywords": ["ecommerce"],
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-yaml": "*",
    "doctrine/dbal": "^2.12.1",
    "iampersistent/money-helpers": "^0.0.3",
    "moneyphp/money": "^3.3.1",
    "psr/container": "^1.0",
    "league/omnipay": "^3",
    "robmorgan/phinx": "^0.12.6"
  },
  "require-dev": {
    "codeception/codeception": "^4.1.18",
    "omnipay/dummy": "dev-master",
    "codeception/module-asserts": "^1.3.1"
  },
  "repositories": {
    "omnipay/dummy": {
      "type": "vcs",
      "url": "git@github.com:americas-mailbox/omnipay-dummy.git"
    }
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "IamPersistent\\SimpleShop\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixture\\": "tests/_support/Fixture",
      "Tests\\Functional\\": "tests/functional",
      "Tests\\Unit\\": "tests/unit"
    }
  }
}
